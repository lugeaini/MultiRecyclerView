<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.chenxulu.widget.recycler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.chenxulu.recycler.GuideActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="cn.chenxulu.recycler.MainActivity">
        </activity>
        <activity android:name="cn.chenxulu.recycler.expand.ExpandActivity">
        </activity>
        <activity android:name="cn.chenxulu.recycler.headerfooter.HeaderFooterActivity">
        </activity>
        <activity android:name="cn.chenxulu.recycler.pull.PullRefreshActivity">
        </activity>
    </application>

</manifest>